{
  "name": "hello-world",
  "version": "0.19.8",
  "description": "used as a template for new reSolve applications",
  "private": true,
  "scripts": {
    "compile": "tsc",
    "build": "tsc && node ./build/run.js build",
    "dev": "tsc && node ./build/run.js dev",
    "start": "tsc && node ./build/run.js start",
    "test": "jest --testMatch=**/test/unit/*.test.js --verbose",
    "test:functional": "tsc && babel-node ./build/run.js test:functional"
  },
  "dependencies": {
    "@babel/node": "^7.5.5",
    "@babel/polyfill": "^7.4.4",
    "@types/node": "^12.7.4",
    "@types/react": "^16.8.8",
    "@types/redux-devtools-extension": "^2.13.2",
    "react": "^16.9.0",
    "react-bootstrap": "0.32.1",
    "react-dom": "^16.9.0",
    "react-helmet": "^5.2.1",
    "redux": "^4.0.4",
    "redux-devtools-extension": "^2.13.8",
    "resolve-command": "0.20.25",
    "resolve-module-auth": "0.20.25",
    "resolve-query": "0.20.25",
    "resolve-readmodel-lite": "0.20.25",
    "resolve-readmodel-mysql": "0.20.25",
    "resolve-redux": "0.20.25",
    "resolve-scheduler-local": "0.20.25",
    "resolve-scripts": "0.20.25",
    "resolve-storage-lite": "0.20.25",
    "styled-components": "^4.3.2",
    "typescript": "^3.3.4000"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-export-default-from": "^7.5.2",
    "@babel/plugin-proposal-export-namespace-from": "^7.5.2",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "7.0.0",
    "babel-jest": "^24.9.0",
    "jest": "^24.9.0",
    "react-test-renderer": "^16.9.0",
    "testcafe": "0.23.3"
  }
}
